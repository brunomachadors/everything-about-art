generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Artwork {
  id          String   @id @default(uuid())
  title       String
  artist      String
  year        Int
  origin      String
  description String
  image       String
  tags        String[]
  createdAt   DateTime @default(now())
}
